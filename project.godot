; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Escape The Bbstacles"
run/main_scene="res://src/screen/SplashScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="escapetheobstacles"
config/features=PackedStringArray("4.2")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/splash_screen/splash_godot.png"
config/icon="res://icon.png"

[audio]

buses/default_bus_layout="res://src/escape_bus_layout.tres"

[autoload]

Data="*res://src/main/data.gd"
Lang="*res://src/main/lang.gd"
Main="*res://src/main/Main.gd"
UserData="*res://src/main/UserData.tscn"
SaveLoad="*res://src/main/saveload.gd"
BackGroundMusic="*res://src/main/BackGroundMusic.tscn"
gd="*res://addons/godot-action/utils_gd_action.tscn"
sp="*res://addons/awesome_splash/utils_splash.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/size/mode=1
window/size/initial_position_type=0
window/size/always_on_top=true
window/size/window_width_override=480
window/size/window_height_override=854
window/stretch/mode="viewport"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/awesome_splash/plugin.cfg", "res://addons/godot-action/plugin.cfg")

[gui]

theme/custom="res://assets/base_theme.tres"
theme/custom_font="res://assets/fonts/game_font.otf"

